	<Description about="urn:mozilla:install-manifest">
		<em:id>node-extension@rawkes.com</em:id>
		<em:version>0.1</em:version>
		<em:type>2</em:type>
		<em:unpack>true</em:unpack>

		<!-- Front End MetaData -->
		<em:name>Node Extension</em:name>
